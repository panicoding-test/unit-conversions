{
    "name": "faisuc/unit-conversions",
    "description": "",
    "keywords": [
        "faisuc",
        "unit-conversions"
    ],
    "homepage": "https://github.com/faisuc/unit-conversions",
    "license": "MIT",
    "authors": [
        {
            "name": "Neil Carlo Sucuangco",
            "email": "necafasu@gmail.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Faisuc\\UnitConversions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Faisuc\\UnitConversions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
